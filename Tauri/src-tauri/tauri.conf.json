{
    "productName": "Monster Controller",
    "version": "0.1.0",
    "identifier": "com.jj.monster",
    "build": {
        "devUrl": "http://localhost:1420",
        "beforeBuildCommand": "pnpm build",
        "frontendDist": "../dist"
    },
    "app": {
        "windows": [
            {
                "title": "Monster Controller",
                "width": 1000,
                "height": 800
            }
        ],
        "security": {
            "csp": null
        }
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": [
            "icons/64x64.ico",
            "icons/128x128.ico",
            "icons/256x256.ico"
        ]
    }
}